<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeTextViewStyle">
        <attr name="gFormat" format="string"/>
    </declare-styleable>

    <declare-styleable name="IconView">
        <attr name="gIcon" format="reference"/>
        <attr name="gText" format="string|reference"/>
        <attr name="gTextSize" format="dimension"/>
        <attr name="gTextColor" format="color|reference"/>
        <attr name="gTextPadding" format="dimension"/>
        <attr name="gIconWidth" format="dimension"/>
        <attr name="gIconHeight" format="dimension"/>
        <attr name="gTextSite" format="string"/>
        <attr name="gLight" format="boolean"/>
        <attr name="gIsAnim" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ControlView">
        <attr name="cIcon" format="reference"/>
        <attr name="cTextSize" format="reference|dimension"/>
        <attr name="cTextColor" format="color|reference"/>
        <attr name="cText" format="string|reference"/>
        <attr name="cTextPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RoundFrameLayout">
        <attr name="rfl_radius" format="dimension" />
        <attr name="topLeftRadius" format="dimension" />
        <attr name="topRightRadius" format="dimension" />
        <attr name="bottomLeftRadius" format="dimension" />
        <attr name="bottomRightRadius" format="dimension" />
    </declare-styleable>
</resources>