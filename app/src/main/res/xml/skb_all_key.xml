            key_code="@string/keycode_del"
            key_icon="@mipmap/delete" />
        &lt;!&ndash; 删除 &ndash;&gt;
        <key
            key_left_padding="@dimen/w_295"
            key_code="@string/keycode_del"
            key_icon="@mipmap/delete" />
    </row>-->
    
    <!-- 第二行 -->
    <row>
        <keys
            labels="A|B|C|D|E|F"
            splitter="|" />
    </row>
	
    <!-- 第三行 -->
    <row>
        <keys
            labels="G|H|I|J|K|L"
            splitter="|" />
    </row>
    <!-- 第四行 -->
    <row>
        <keys
            labels="M|N|O|P|Q|R"
            splitter="|" />
    </row>
    <!-- 第五行 -->
    <row>
        <keys
            labels="S|T|U|V|W|X"
            splitter="|" />
    </row>

    <!-- 第六行 -->
    <row>
        <keys
            labels="Y|Z|0|1|2|3"
            splitter="|" />
    </row>
    
    <row>
        <keys
            labels="4|5|6|7|8|9"
            splitter="|" />
        <!--<key
            key_label="Z" />
        &lt;!&ndash; 123数字切换 &ndash;&gt;
