<?xml version="1.0" encoding="utf-8"?>
<!-- 全键盘 -->
<Keyboard
    bg_res="@android:color/transparent"
    key_left_padding="@dimen/w_10"
    key_bg_res="@drawable/key_bg_rectangle"
    key_press_res="@drawable/softkey_bg_press2"
    key_select_res="@drawable/softkey_bg_select2"
    key_text_color="#b8bbbf"
    key_text_size="@dimen/w_35"
    key_top_padding="@dimen/h_10"
    key_height="@dimen/h_85"
    key_width="@dimen/w_85"
    start_pos_x="0"
    start_pos_y="0"
    left_right_move="false"
    top_bottom_move="false" >

    <!-- 第一行 -->
    <row>
         <!-- 删除 -->
        <key
            key_code="@string/keycode_del"
            key_icon="@mipmap/delete" />
        <!-- 删除 -->
        <key
            key_left_padding="@dimen/w_295"
            key_code="@string/keycode_del"
            key_icon="@mipmap/delete" />
    </row>
    
    <!-- 第二行 -->
    <row>
        <keys
            labels="A|B|C|D|E"
            splitter="|" />
    </row>
	
    <!-- 第三行 -->
    <row>
        <keys
            labels="F|G|H|I|J"
            splitter="|" />
    </row>
	
    <!-- 第四行 -->
    <row>
        <keys
            labels="U|V|W|X|Y"
            splitter="|" />
    </row>
    
    <row>
        <key
            key_label="Z" />
      <!--  &lt;!&ndash; 123数字切换 &ndash;&gt;
        <key
            key_code="250"
            key_icon="@drawable/quit_skb" />-->
         <!-- 删除 -->
        <key
            key_code="@string/keycode_del"
            key_icon="@mipmap/delete" />
    </row>
    
</Keyboard>